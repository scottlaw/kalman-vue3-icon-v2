{
  "name": "kalman-vue3-icon-v2",
  "version": "0.1.0",
  "description": "Kalman Vue Icon V2, Automation workflow with Figma",
  "files": [
    "dist/*",
    "src/index.d.ts"
  ],
  "main": "./dist/index.umd.ts",
  "module": "./dist/index.es.ts",
  "typings": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.ts",
      "require": "./dist/index.umd.ts"
    }
  },
  "keywords": [
    "kalman",
    "icons"
  ],
  "scripts": {
    "dev": "vite",
    "generate": "sudo rm -rf src/icons && node bin/build.js",
    "build": "vue-tsc --noEmit && vite build",
    "build:bundle": "vue-tsc --noEmit && vite build --config bundle.config.ts",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.3.3",
    "cheerio": "^1.0.0-rc.11",
    "svgo": "^1.3.0",
    "typescript": "^4.5.4",
    "uppercamelcase": "^3.0.0",
    "vite": "^2.9.9",
    "vite-plugin-dts": "^1.2.0",
    "vue-tsc": "^0.35.0"
  },
  "config": {
    "framework": "vue",
    "style": "fill"
  }
}
